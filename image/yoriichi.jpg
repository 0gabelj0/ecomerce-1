        function copyDivToClipboard() {
            var range = document.createRange();
            range.selectNode(document.getElementById("cupom_de_desconto"));
            window.getSelection().removeAllRanges();
            window.getSelection().addRange(range);
            document.execCommand("copy");
            window.getSelection().removeAllRanges();
            alert('CUPOM COPIADO! Na etapa de compra e fechamento do pedido, procure pelo campo de cupom e insira o código copiado. Pronto! O desconto será aplicado!')
        }
        
        cookies = localStorage.getItem('cookies');
        const el = document.getElementById('cookiesInfo');

        setInterval(() => {
            if (cookies != 1) {
                el.classList.add('ativo');
                localStorage.setItem('cookies', 1);
            }
        }, 1000)

        el.addEventListener('click', () => {
            el.classList.add('desativo')
            setInterval(() => {
                el.style.display = 'none';
            }, 1000)
        })
    </script>
